{
    "name": "Facebook-Red-Pill",
    "manifest_version": 2,
    "version": "1.0",
    "background": {
        "scripts": ["background.js"]
      },
    "options_page": "options.html",
    "description": "Extension para manejar el uso de la informacion dentro de Facebook",
    "icons": {
        "16": "icono.png",
        "48": "icono.png",
        "128": "icono.png"
      },
    "browser_action": {
	"default_icon": "icono.png",
    "default_popup": "popup.html"
      },
    "permissions": [
	    "tabs"
	    ]
}
